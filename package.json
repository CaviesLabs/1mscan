{
  "name": "seitrace-frontend",
  "version": "1.0.0",
  "private": false,
  "homepage": "https://github.com/CaviesLabs/seitrace-frontend",
  "type": "module",
  "scripts": {
    "dev:turbo": "NODE_ENV=development NEXT_PUBLIC_APP_ENV=local-dev EXPERIMENTAL_TURBO=true ./node_modules/.bin/next dev --turbo",
    "dev:local": "NODE_ENV=development NEXT_PUBLIC_APP_ENV=local-dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next dev",
    "dev:development": "NODE_ENV=development NEXT_PUBLIC_APP_ENV=dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next dev",
    "dev:production": "NODE_ENV=development NEXT_PUBLIC_APP_ENV=prod NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next dev",
    "build:local": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=local-dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next build",
    "build:development": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next build",
    "build:staging": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=staging NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next build",
    "build:production": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=prod NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next build",
    "build:svg": "icons build -i ./icons -o ./public/icons --optimize",
    "build:theme": "chakra-cli tokens theme/index.ts",
    "start:local": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=local-dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next start",
    "start:development": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=dev NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next start",
    "start:staging": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=staging NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next start",
    "start:production": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=prod NEXT_PUBLIC_GIT_COMMIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_GIT_TAG=$(git describe --tags --abbrev=0) ./node_modules/.bin/next start",
    "lint:tsc": "tsc -p ./tsconfig.json",
    "lint:biome": "biome check",
    "lint:biome:fix": "biome check --write",
    "svg:format": "svgo -r ./icons",
    "theme:watch": "chakra-cli tokens theme/index.ts --watch",
    "sitemap:dev": "NEXT_PUBLIC_APP_ENV=dev next-sitemap --config ./tools/scripts/sitemap.config.js",
    "sitemap:prod": "NEXT_PUBLIC_APP_ENV=prod next-sitemap --config ./tools/scripts/sitemap.config.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
